{
    "name": "rawrkats/laravel-joypixels",
    "type": "library",
    "description": "Laravel helper for Joypixels, Formerly EmojiOne",
    "keywords": [
        "ChristofferOK",
        "LaravelEmojiOne",
        "laravel",
        "emoji",
        "emojione",
        "joypixels",
        "LaravelJoyPixels",
        "rawr"
    ],
    "homepage": "https://github.com/rawrkats/LaravelJoypixels",
    "license": "MIT",
    "authors": [
        {
            "name": ":Christoffer Korvald",
            "email": "korvald@gmail.com",
            "homepage": "https://christofferok.com",
            "role": "Developer"
        },
        {
            "name": "Kat Hoff",
            "email": "rawrkats@me.com",
            "homepage": "https://kat.land",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "joypixels/emoji-toolkit": "^5.0",
        "illuminate/support": "~5.1"
    },
    "suggest": {
        "joypixels/assets": "If you want to serve the emojione assets yourself"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "RawrSocial\\LaravelJoyPixels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RawrSocial\\LaravelJoyPixels\\": "tests"
        }
    },
    "scripts": {},
    "extra": {
        "laravel": {
            "providers": [
                "RawrSocial\\LaravelJoyPixels\\LaravelJoyPixelsServiceProvider"
            ],
            "aliases": {
                "LaravelEmojiOne": "RawrSocial\\LaravelJoyPixels\\LaravelJoyPixelsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
